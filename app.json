{
  "expo": {
    "name": "2T Delivery",
    "slug": "2tdelivery",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00BF68"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doist.delivery"
    },
    "android": {
      "package": "com.doist.delivery"
    },
    "extra": {
      "EXPO_API_URL": "https://smerca-api-staging.herokuapp.com/api/v1"
    }
  }
}
